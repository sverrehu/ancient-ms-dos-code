.AUTODEPEND

!if !$d(DEBUG)
DEBMODE=/t
!else
DEBMODE=/v
!endif

ALL:
   MAKE ED.COM
   MAKE EDSETUP.COM

ED.COM: ED.INC ED.OBJ STACK.OBJ FILELOW.OBJ FILE.OBJ LINE.OBJ ENVIRO.OBJ EDIT.OBJ EDITFUNC.OBJ MESSAGE.OBJ MOUSE.OBJ BLOCK.OBJ FIND.OBJ PICK.OBJ CHOOSEF.OBJ \ASM\ASMLIB\ASMLIB.LIB
   TLINK /x $(DEBMODE) ED STACK FILELOW FILE LINE ENVIRO EDIT EDITFUNC MESSAGE MOUSE BLOCK FIND PICK CHOOSEF, ED, , \ASM\ASMLIB\ASMLIB

EDSETUP.COM: ED.INC EDSETUP.OBJ
   TLINK /x $(DEBMODE) EDSETUP, EDSETUP, , \ASM\ASMLIB\ASMLIB

.ASM.OBJ:
   TASM /ml /m2 /zi $<
